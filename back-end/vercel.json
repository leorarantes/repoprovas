{
    "version": 2,
    "builds": [
        {
            "src": "build.js",
            "use": "@vercel/node",
            "config": { "command": "npm run build" }
        },
        {
            "src": "dist/src/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/index.js"
        }
    ]
}
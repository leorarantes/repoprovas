{
    "name": "repoprovas",
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": { "command": "npm run build" }
        },
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": { "command": "npm run start" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}
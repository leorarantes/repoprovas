{
    "name": "repoprovas",
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": { "command": "npm i" }
        },
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": { "command": "rm -rf ./dist && tsc" }
        },
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": { "command": "ls -l" }
        },
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": { "command": "node dist/src/index.js" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}